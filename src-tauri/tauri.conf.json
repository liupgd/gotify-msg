{
  "productName": "gotify-msg",
  "version": "0.1.0",
  "identifier": "com.gotify.client",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "main",
        "title": "Gotify 客户端",
        "url": "/index.html",
        "width": 600,
        "height": 500,
        "resizable": true,
        "fullscreen": false,
        "minimizable": true,
        "maximizable": true,
        "closable": true,
        "skipTaskbar": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}
